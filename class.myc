{pure Int}
(= pure 0)

{pure Float}
(= pure 0.0)

{zero Int}
(= zero {Int pure})

{pure-pair (For {(Class pure) a} [a a])}
(= pure-pair [pure pure])

-----

(= Math (Class + *))
{+ (For a (=> a a a))}
{* (For a (=> a a a))}

{+ (=> Int Int Int)}
(= + i32.add)
{* (=> Int Int Int)}
(= * i32.mul)

{+ (=> Float Float Float)}
(= + f32.add)
{* (=> Float Float Float)}
(= * f32.mul)

{calc (For {Math a} (=> a a))}
(= calc (-> x (+ x (* x x))))

(calc 1)

