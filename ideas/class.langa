(= not
   (: (=> Bool Bool)
      (? (-> `true` `false`)
         (-> `false` `true`))))

(= not
   (: (=> Int Int)
      (? (-> 1 0)
         (-> 0 1))))

(= not
   (: (=> Float Float)
      (? (-> 1.0 0.0)
         (-> 0.0 1.0))))

(= maybe-not
   (A (: Type a)
      (: (=> (Maybe a) (Maybe a))
         (? (-> [`just` x] [`just` (not x)]) -- ?
            (-> `none` `none`)))))
